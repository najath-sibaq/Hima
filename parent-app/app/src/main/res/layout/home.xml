<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:gravity="right"
		android:orientation="horizontal">
		<TextView
			android:id="@+id/textview37"
			android:layout_width="wrap_content"
			android:layout_height="0dp"
			android:padding="8dp"
			android:text="TextView"
			android:textSize="12sp"
			android:textColor="#000000" />
		<TextView
			android:id="@+id/textview36"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:padding="8dp"
			android:text="TextView"
			android:textSize="12sp"
			android:textColor="#000000" />
		<TextView
			android:id="@+id/id"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="TextView"
			android:textSize="12sp"
			android:textColor="#000000" />
		<TextView
			android:id="@+id/textview1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Log out"
			android:textSize="14sp"
			android:textStyle="bold"
			android:textColor="#000000" />
	</LinearLayout>
	<ScrollView
		android:id="@+id/vscroll1"
		android:layout_width="match_parent"
		android:layout_height="match_parent">
		<LinearLayout
			android:id="@+id/linear62"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/linear3"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/linear4"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/linear18"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/linear20"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<de.hdodenhof.circleimageview.CircleImageView
								android:id="@+id/circleimageview1"
								android:layout_width="70dp"
								android:layout_height="100dp"
								android:src="@drawable/porfilew"
								app:civ_border_width="3dp"
								app:civ_border_color="#008DCD"
								app:civ_circle_background_color="#FFFFFF"
								app:civ_border_overlay="true" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear19"
							android:layout_width="200dp"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:orientation="vertical"
							android:weightSum="1"
							android:layout_weight="1">
							<TextView
								android:id="@+id/textview25"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:text="Child name"
								android:textSize="12sp"
								android:textColor="#000000"
								android:translationY="10.0dp" />
							<TextView
								android:id="@+id/name"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingRight="8dp"
								android:paddingBottom="8dp"
								android:text="Loading........"
								android:textSize="25sp"
								android:textStyle="bold"
								android:textColor="#000000" />
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear21"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/linear22"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/linear23"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/date"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_margin="16dp"
							android:padding="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/textview27"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:gravity="center_horizontal|center_vertical"
								android:text="TextView"
								android:textSize="16sp"
								android:textStyle="bold"
								android:textColor="#616161"
								android:layout_gravity="center_horizontal|center_vertical" />
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear25"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/linear26"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/linear27"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/screentime"
							android:layout_width="200dp"
							android:layout_height="120dp"
							android:layout_marginRight="5dp"
							android:paddingLeft="8dp"
							android:paddingTop="25dp"
							android:paddingRight="8dp"
							android:paddingBottom="25dp"
							android:background="#383E7E"
							android:gravity="center_vertical"
							android:orientation="vertical">
							<TextView
								android:id="@+id/screentimee"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:text="Loading..."
								android:textSize="20sp"
								android:textStyle="bold"
								android:textColor="#FFFFFF" />
							<TextView
								android:id="@+id/textview29"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:gravity="center_horizontal|center_vertical"
								android:text="Total Screen Time"
								android:textSize="14sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/location"
							android:layout_width="match_parent"
							android:layout_height="120dp"
							android:paddingLeft="4dp"
							android:paddingTop="20dp"
							android:paddingRight="4dp"
							android:paddingBottom="20dp"
							android:background="#383E7E"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/imageview2"
								android:layout_width="20dp"
								android:layout_height="40dp"
								android:layout_marginBottom="10dp"
								android:src="@drawable/location"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview31"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="4dp"
								android:paddingTop="3dp"
								android:paddingRight="4dp"
								android:paddingBottom="8dp"
								android:gravity="center_horizontal|center_vertical"
								android:text="Current Location"
								android:textSize="14sp"
								android:textColor="#FFFFFF"
								android:layout_gravity="center_horizontal|center_vertical" />
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear34"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/linear35"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/con3"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_margin="8dp"
						android:padding="15dp"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/linear37"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:orientation="horizontal">
							<LinearLayout
								android:id="@+id/linear41"
								android:layout_width="wrap_content"
								android:layout_height="match_parent"
								android:padding="8dp"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="horizontal">
								<ImageView
									android:id="@+id/imageview5"
									android:layout_width="25dp"
									android:layout_height="40dp"
									android:src="@drawable/appusage"
									android:scaleType="fitCenter" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear44"
								android:layout_width="90dp"
								android:layout_height="match_parent"
								android:gravity="center_vertical"
								android:orientation="horizontal"
								android:layout_weight="1">
								<TextView
									android:id="@+id/textview32"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:paddingLeft="10dp"
									android:text="App Usage"
									android:textSize="18sp"
									android:textStyle="bold"
									android:textColor="#000000" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear47"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="horizontal">
								<ImageView
									android:id="@+id/imageview8"
									android:layout_width="25dp"
									android:layout_height="50dp"
									android:src="@drawable/arroww"
									android:scaleType="fitCenter" />
							</LinearLayout>
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear58"
							android:layout_width="match_parent"
							android:layout_height="1dp"
							android:layout_marginLeft="25dp"
							android:layout_marginRight="25dp"
							android:padding="8dp"
							android:background="#455A64"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/linear49"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:orientation="horizontal">
							<LinearLayout
								android:id="@+id/linear50"
								android:layout_width="wrap_content"
								android:layout_height="match_parent"
								android:padding="8dp"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="horizontal">
								<ImageView
									android:id="@+id/imageview10"
									android:layout_width="25dp"
									android:layout_height="40dp"
									android:src="@drawable/manageapps"
									android:scaleType="fitCenter" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear51"
								android:layout_width="90dp"
								android:layout_height="match_parent"
								android:gravity="center_vertical"
								android:orientation="horizontal"
								android:layout_weight="1">
								<TextView
									android:id="@+id/textview33"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:paddingLeft="10dp"
									android:text="Manage Apps"
									android:textSize="18sp"
									android:textStyle="bold"
									android:textColor="#000000" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear52"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="horizontal">
								<ImageView
									android:id="@+id/imageview11"
									android:layout_width="25dp"
									android:layout_height="50dp"
									android:src="@drawable/arroww"
									android:scaleType="fitCenter" />
							</LinearLayout>
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear57"
							android:layout_width="match_parent"
							android:layout_height="1dp"
							android:layout_marginLeft="25dp"
							android:layout_marginRight="25dp"
							android:padding="8dp"
							android:background="#37474F"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/linear53"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:orientation="horizontal">
							<LinearLayout
								android:id="@+id/linear54"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="horizontal">
								<ImageView
									android:id="@+id/imageview12"
									android:layout_width="25dp"
									android:layout_height="40dp"
									android:src="@drawable/deviceinformation"
									android:scaleType="fitCenter" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear55"
								android:layout_width="90dp"
								android:layout_height="match_parent"
								android:gravity="center_vertical"
								android:orientation="horizontal"
								android:layout_weight="1">
								<TextView
									android:id="@+id/textview34"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:paddingLeft="10dp"
									android:text="Device Information"
									android:textSize="18sp"
									android:textStyle="bold"
									android:textColor="#000000" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear56"
								android:layout_width="wrap_content"
								android:layout_height="match_parent"
								android:padding="8dp"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="horizontal">
								<ImageView
									android:id="@+id/imageview13"
									android:layout_width="25dp"
									android:layout_height="25dp"
									android:src="@drawable/arroww"
									android:scaleType="fitCenter" />
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear60"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/linear61"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="15dp"
					android:layout_marginRight="15dp"
					android:padding="8dp"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/block"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/textview35"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:text="Block All Social Media Apps"
							android:textSize="18sp"
							android:textStyle="bold"
							android:textColor="#FFFFFF" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
